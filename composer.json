{
    "name": "strategio/framework",
    "description": "SFramework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jiří Zapletal",
            "email": "jz@strategio.digital"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "nette/di": "^3.0",
        "nette/utils": "^3.2",
        "nette/schema": "^1.2",
        "nette/security": "^3.1",
        "tracy/tracy": "^2.9",
        "symfony/routing": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/console": "^6.1",
        "symfony/cache": "^6.1",
        "symfony/uid": "^6.1",
        "doctrine/orm": "^2.13.3",
        "doctrine/dbal": "^3.4.5",
        "doctrine/annotations": "^1.13.3",
        "doctrine/migrations": "^3.5",
        "lcobucci/jwt": "^4.2",
        "aws/aws-sdk-php": "^3.240",
        "nette/mail": "^3.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse -l 8 src bin --memory-limit 1G",
        "neon": "vendor/bin/neon-lint config",
        "analyse": [
            "vendor/bin/phpstan analyse -l 8 src bin --memory-limit 1G",
            "vendor/bin/neon-lint config"
        ]
    }
}
